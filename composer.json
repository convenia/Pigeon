{
    "name": "convenia/pigeon",
    "license": "mit",
    "type": "library",
    "description": "",
    "keywords": ["pigeon", "rabbitmq", "laravel", "message", "queue", "amqp"],
    "authors": [
        {
            "name": "Kauan Sousa",
            "email": "kauanslr.ks@gmail.com",
            "role": "Maintainer"
        },
        {
            "name": "Leonardo Lemos",
            "email": "leonardo.lemos@convenia.com.br",
            "role": "Maintainer"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "ext-sockets": "*",
        "php-amqplib/php-amqplib": "^3.1"
    },
    "require-dev": {
        "ext-curl": "*",
        "squizlabs/php_codesniffer": "^2.5",
        "orchestra/testbench": "^8.19"
    },
    "autoload": {
        "psr-4": {
            "Convenia\\Pigeon\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Convenia\\Pigeon\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Convenia\\Pigeon\\PigeonServiceProvider"
            ],
            "aliases": {
                "Pigeon": "Convenia\\Pigeon\\Facade\\Pigeon"
            }
        }
    }
}
