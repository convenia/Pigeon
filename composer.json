    "require": {
        "php": ">=7.1.3",
        "ext-json": "*",
        "ext-sockets": "*",
        "php-amqplib/php-amqplib": ">=2.9",
        "laravel/framework": "^8.0",
        "webpatser/laravel-uuid": "^3.0"
    },
    "require-dev": {
        "ext-curl": "*",
        "phpunit/phpunit": "^9.4.2",
        "squizlabs/php_codesniffer": "^2.5",
        "orchestra/testbench": "6.0.*",
        "mockery/mockery": "1.3.*"
    },
    "autoload": {
        "psr-4": {
            "Convenia\\Pigeon\\": "src/"
        }
